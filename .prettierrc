{
  "trailingComma": "es5",
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^react$",
    "^next(/.*)?$",
    "next-themes",
    "@/*",
    "<THIRD_PARTY_MODULES>"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
